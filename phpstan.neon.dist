
#210215 phpstan.neon.dist - larastan config
parameters:

    paths:
        - app

    # The level 9 is the highest level
    level: 9

    ignoreErrors:
        - '#Call to an undefined static method [a-zA-Z0-9\\_]#'

    excludePaths:
        - storage
        - vendor

    checkMissingIterableValueType: false
    checkGenericClassInNonGenericObjectType: false

